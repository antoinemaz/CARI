                    {{else:}}
                        Documents
                    {{pass}}
                        
                {{if rowDossier is not None:}}        
                    </a>
                {{pass}}
            </span></li>
            
            {{if initDossier == True:}}
                <li><span>
                    
                    {{if rowDossier is not None:}}
                    <a {{if finalDemandeView == True:}}class="currentCase" {{pass}} href="../projet/finalisationDemande?idDossier={{=request.vars.idDossier}}">
                    {{pass}}
                            Finalisation de la demande
                            
                    {{if rowDossier is not None:}}
                        </a>
                    {{pass}}
                    
                 </span></li>
            {{pass}}
        </ul>
    </div>
